2.
first_num = input("write first number: ")
second_num = input("write second number: ")

if(first_num > second_num):
    if(int(first_num) % int(second_num) == 0):
        print("არის ჯერადი")
elif(first_num < second_num):
    if(int(second_num) % int(first_num) == 0):
        print("არის ჯერადი")
elif(first_num == second_num):
    print("არის ჯერადი")
